{
  "name": "react-ts-emotion-starter",
  "version": "1.0.0",
  "description": "React, Typescript, Emotion, Jest, Webpack boilerplate",
  "main": "index.js",
  "author": "Cameron Bacciarini <cbacciar@gmail.com>",
  "scripts": {
    "npm:current": "ncu -u",
    "clean": "rimraf dist build dist coverage docs .out",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx)\"",
    "types": "tsc",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "build": "npm run clean && cross-env NODE_ENV=production webpack -p --config webpack/webpack.prod.config.js && npm run docs",
    "format": "npm run pretty -- --write",
    "format:check": "npm run pretty -- --list-different",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -p --config webpack/webpack.dev.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --collect-coverage",
    "docs": "typedoc --out docs src/components",
    "build:analysis": "npm run clean && cross-env NODE_ENV=analysis webpack -p --config webpack/webpack.prod.config.js",
    "validate": "npm run types && npm run format:check && npm run lint && npm run build"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.0",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.27",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "jest-config": "^25.1.0",
    "jest-fetch-mock": "^3.0.1",
    "jsdom": "^16.1.0",
    "lint-staged": "^10.0.7",
    "mini-css-extract-plugin": "^0.9.0",
    "modernizr-webpack-plugin": "^1.0.7",
    "npm-check-updates": "^4.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.4",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typedoc": "^0.16.9",
    "typescript": "^3.7.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@hot-loader/react-dom": "^16.11.0",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.19"
  }
}
